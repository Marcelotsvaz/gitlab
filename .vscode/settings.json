{
	"terminal.integrated.profiles.linux": {
		"fish": {
			"path": "/usr/bin/fish",
			"args": [ "-C", "source '${workspaceFolder}/scripts/setupEnv.fish'" ],
		},
	},
	
	"terminal.integrated.automationProfile.linux": {
		"path": "/usr/bin/fish",
		"args": [ "-C", "source '${workspaceFolder}/scripts/setupEnv.fish'" ],
	},
	
	"files.associations":
	{
		"**/templates/*.yml": "yaml",
	},
	
	"yaml.schemas": {
		"https://gitlab.com/gitlab-org/gitlab/-/raw/master/app/assets/javascripts/editor/schema/ci.json": "/templates/*.yml",
	},
}